<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">

<struts>
	<!-- 服务器系统入口 -->
	<package name="cp" namespace="/cp" extends="struts-default">
		  <action name="hybjShowAction_*" class="cn.hybj.web.action.HybjShowAction" method="{1}">
	    	<result name="hs">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="yt">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="yp">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="bst">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="aqy">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="mg">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="tx">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="tysx">
	    		/WEB-INF/page/show/hsIndex.jsp
	    	</result>
	    	<result name="fileImport">
	    		/WEB-INF/page/show/fileImport.jsp
	    	</result>
	    	<result name="reportAdd">
	    		/WEB-INF/page/show/reportAdd.jsp
	    	</result>
			  <result name="showCG">
	    		/WEB-INF/page/menu/showCG.jsp
	    	</result>
			  <result name="xq">
				  /WEB-INF/page/show/showXQ.jsp
			  </result>
			  <result name="demandAdd">
				  /WEB-INF/page/show/demandAdd.jsp
			  </result>
	    </action>
	    
	     <action name="hybjReportAction_*" class="cn.hybj.web.action.HybjReportAction" method="{1}">
	    	<result type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">inputStream</param>
			</result>
			<result name="fileImport">
	    		/WEB-INF/page/show/fileImport.jsp
	    	</result>
			<result name="importResult">
	    		/WEB-INF/page/show/importResult.jsp
	    	</result>
	    </action>

		<!--需求-->
		<action name="hybjDemandAction_*" class="cn.hybj.web.action.HybjDemandAction" method="{1}">
			<result type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">inputStream</param>
			</result>
			<result name="demandAdd">
				/WEB-INF/page/show/demandAdd.jsp
			</result>
		</action>
	</package>
</struts>
